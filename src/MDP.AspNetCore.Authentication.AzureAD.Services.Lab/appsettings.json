{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.*": "Warning",
      "System.*": "Warning"
    }
  },

  "Authentication": {
    "AzureAD.Services": {
      "Scheme": "AzureAD.Services",
      "Header": "Authorization",
      "Prefix": "Bearer",
      "Instance": "https://login.microsoftonline.com/",
      "TenantId": "xxxxx",
      "ClientId": "xxxxx"
    }
  }
}